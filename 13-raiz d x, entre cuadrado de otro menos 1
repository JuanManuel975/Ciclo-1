/*  Realice un programa que calcule el valor que toma la siguiente funci√≥n para unos valores dados de x e y: f(x,y) = sqrt(x) / (pow(y,2)-1) */

#include <iostream>
#include<math.h>
#include<stdio.h>

using namespace std;

int main()
{
	double x, y, f = 0;

    cout << "\nIngrese una variable:"; cin >> x;
	cout << "\nIngrese otra variable variable:"; cin >> y;

	x = sqrt(x);
	y = (y = pow(y, 2) - 1);
	f = x / y;

	cout << "\nEl resultado de la ecucacion es: " << f;

		return 0;
}
