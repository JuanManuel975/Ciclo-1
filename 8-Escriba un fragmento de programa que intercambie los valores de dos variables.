#include <iostream>
using namespace std;

int main()
{

    int x,y,z,aux;

    cout << "\nDigete el valor de x: "; cin >> x;
    cout << "\nDigete el valor de y: "; cin >> y;
    cout << "\nDigete el valor de z: "; cin >> z;
    
    aux = x;
    x = y;
    y = z;
    z = aux;
    
    cout << "\nEl numero valor de x es: " << x;
    cout << "\nEl numero valor de y es: " << y;
    cout << "\nEl numero valor de z es: " << z;

    return 0;
}

